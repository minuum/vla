{
  "hidden_size": 512,
  "use_lite_mode": true,
  "image_backbone": "efficientnet_v2_s",
  "text_model": "klue/roberta-base",
  "freeze_image_backbone": false,
  "freeze_text_encoder": false,
  "dropout": 0.1,

  "learning_rate": 1e-4,
  "weight_decay": 0.01,
  "batch_size": 2,
  "sequence_length": 18,
  "max_epochs": 1,
  "scheduler": "cosine",
  "num_workers": 2,

  "train_data_dir": "${MOBILE_VLA_DATA_DIR}",
  "val_data_dir": "${MOBILE_VLA_DATA_DIR}",

  "scenario_weights": {
    "1box_vert_left": 1.0,
    "1box_vert_right": 1.0,
    "1box_hori_left": 1.2,
    "1box_hori_right": 1.1,
    "2box_vert_left": 1.5,
    "2box_vert_right": 1.4,
    "2box_hori_left": 1.8,
    "2box_hori_right": 1.6
  }
}


